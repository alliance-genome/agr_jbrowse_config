{
  "tracks": [
    {
      "type": "FeatureTrack",
      "trackId": "rat_all_genes",
      "name": "All Genes",
      "assemblyNames": [
        "Rattus_norvegicus"
      ],
      "adapter": {
        "type": "NCListAdapter",
        "rootUrlTemplate": {
          "locationType": "UriLocation",
          "uri": "https://s3.amazonaws.com/agrjbrowse/docker/5.3.0/RGD/rat/tracks/All_Genes/{refseq}/trackData.jsonz"
        }
      },
      "displays": [
        {
          "type": "LinearBasicDisplay",
          "displayId": "rat_all_genes-LinearBasicDisplay",
          "renderer": {
            "type": "SvgFeatureRenderer",
            "color3": "#30332E",
            "color1": "jexl:get(feature,'strand')<0?'teal':'darkviolet'"
          }
        },
        {
          "type": "LinearArcDisplay",
          "displayId": "rat_all_genes-LinearArcDisplay"
        }
      ]
    },
