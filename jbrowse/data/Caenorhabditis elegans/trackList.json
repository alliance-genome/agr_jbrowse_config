{
   "names" : {
      "type" : "Hash",
      "url" : "https://s3.amazonaws.com/agrjbrowse/docker/5.3.0/WormBase/c_elegans_PRJNA13758/names/"
   },
   "tracks" : [
      {
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "key" : "Reference sequence",
         "label" : "DNA",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/WormBase/c_elegans_PRJNA13758/seq/{refseq_dirpath}/{refseq}-",
         "seqType" : "dna",
         "type" : "SequenceTrack",
         "compress" : 1,
         "chunkSize" : 80000
      },
      {
         "chunkSizeLimit" : 5000000,
         "maxHeight" : 3000,
         "unsafeMouseover" : true,
         "unsafePopup" : true,
         "glyph" : "{simpleVariationGlyph}",
         "onClick" : {
            "label" : "{htpVariantsBalloon}",
            "content" : "{htpVariantsBalloon}",
            "title" : " ",
            "action" : "contentDialog"
         },
         "style" : {
             "color" : "{htpVariationColor}",
             "height" : 8
         },
         "metadata" : {
            "about" : "High Throughput variants. Features are colored according to the Variant Effect Predictor's predicted impact: red for high, purple for modifier, gold/yellow for medium, and cyan for low."
         },
         "label" : "High Throughput Variants",
         "type" : "CanvasVariants",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/VCF/5.3.0/HTPOSTVEPVCF_WB_latest.vcf.gz",
         "storeClass" : "JBrowse/Store/SeqFeature/VCFTabix",
         "key" : "High Throughput Variants"
      },
      {
         "label" : "All Genes",
         "maxHeight" : 3000,
         "trackType" : "CanvasFeatures",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "glyph" : "{geneGlyph}",
         "impliedUTRs" : true,
         "onClick" : {
            "url" : "{curieUrl}",
            "title" : "Click to open in a full window:",
            "label" : "Feature at AGR",
            "action" : "iframeDialog"
         },
         "style" : {
            "color" : "{geneColor}",
            "height" : "{geneHeight}",
            "label" : "{geneLabel}",
            "className" : "feature"
         },
         "compress" : 1,
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/docker/5.3.0/WormBase/c_elegans_PRJNA13758/tracks/All_Genes/{refseq}/trackData.jsonz",
         "type" : "CanvasFeatures",
         "key" : "All Genes"
      },
      {
         "chunkSizeLimit" : 5000000,
         "maxHeight" : 3000,
	 "unsafeMouseover" : true,
	 "unsafePopup" : true,
	 "style" : {
            "color" : "{variationColor}",
             "height" : 8,
	     "description" : "{variationDescription}",
             "label" : "{variationLabel}"
         },
         "glyph" : "{variationGlyph}",
	 "onClick" : {
            "label" : "{variantsBalloon}",
            "content" : "{variantsBalloon}",
            "title" : "{name}",
            "action" : "contentDialog"
         },
	 "metadata" : {
            "about" : "Phenotypic variants. Features are colored according to the Variant Effect Predictor's predicted impact: red for high, purple for modifier, gold/yellow for medium, and cyan for low."
	 },
         "label" : "Variants",
         "type" : "CanvasVariants",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/VCF/5.3.0/worm-latest.vcf.gz",
         "storeClass" : "JBrowse/Store/SeqFeature/VCFTabix",
         "key" : "Variants"
      },
      {
         "label" : "UCSC Conservation",
         "variance_band" : 0,
         "unsafeMouseover" : true,
         "autoscale" : "local",
         "type" : "JBrowse/View/Track/Wiggle/XYPlot",
         "urlTemplate" : "https://s3.amazonaws.com/wormbase-modencode/UCSC/ce11.phastCons26way.bw",
         "unsafePopup" : true,
         "key" : "UCSC Conservation",
         "metadata" : {
            "shortInfo" : "UCSC Nematode conservation calculated across nematode 26 species. For more information about these data, see the <a href='http://hgdownload.soe.ucsc.edu/goldenPath/ce11/phastCons26way/'>README at UCSC</a>. "
         },
         "style" : {
            "height" : 20,
            "pos_color"  : "grey"
         },
         "storeClass" : "JBrowse/Store/SeqFeature/BigWig"
      }
   ],
   "dataset_id" : "worm",
   "plugins" : [
      "NeatHTMLFeatures",
      "HideTrackLabels"
   ],
   "include" : [
      "../functions.conf"
   ],
   "formatVersion" : 1
}
