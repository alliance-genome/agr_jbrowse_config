{
   "plugins" : [
      "NeatHTMLFeatures",
      "HideTrackLabels"
   ],
   "include" : [
      "../functions.conf"
   ],
   "tracks" : [
      {
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "chunkSize" : 80000,
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/human/seq/{refseq_dirpath}/{refseq}-",
         "label" : "DNA",
         "compress" : 1,
         "type" : "SequenceTrack",
         "key" : "Reference sequence"
      },
      {
         "glyph" : "JBrowse/View/FeatureGlyph/Gene",
         "impliedUTRs" : true,
         "onClick" : {
            "url" : "{curieUrl}",
            "title" : "Click to open in a full window:",
            "label" : "Feature at AGR",
            "action" : "iframeDialog"
         },
         "style" : {
            "color" : "{geneColor}",
            "height" : "{geneHeight}",
            "label" : "{geneLabel}",
            "className" : "feature"
         },
         "key" : "All Genes",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : "CanvasFeatures",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/docker/4.0.0/human/tracks/All Genes/{refseq}/trackData.jsonz",
         "compress" : 1,
         "type" : "CanvasFeatures",
         "label" : "All Genes",
         "maxHeight" : 3000
      },
      {
         "label" : "UCSC Conservation",
         "variance_band" : 0,
         "unsafeMouseover" : true,
         "autoscale" : "local",
         "type" : "JBrowse/View/Track/Wiggle/XYPlot",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/UCSC/hg38.phastCons20way.bw",
         "unsafePopup" : true,
         "key" : "UCSC Conservation",
         "metadata" : {
            "shortInfo" : "UCSC conservation calculated across 20 species (including mouse and rat). For more information about these data, see the <a href='http://hgdownload.cse.ucsc.edu/goldenPath/hg38/phastCons20way/'>README at UCSC</a>. "
         },
         "style" : {
            "height" : 20,
            "pos_color"  : "grey"
         },
         "storeClass" : "JBrowse/Store/SeqFeature/BigWig"
      }
   ],
   "names" : {
      "url" : "https://s3.amazonaws.com/agrjbrowse/docker/3.2.0/human/names/",
      "type" : "Hash"
   },
   "dataset_id" : "human",
   "formatVersion" : 1
}
