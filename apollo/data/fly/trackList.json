{
   "plugins" : [
      "NeatHTMLFeatures",
      "HideTrackLabels"
   ],
   "names" : {
      "url" : "https://s3.amazonaws.com/agrjbrowse/docker/3.0.0/FlyBase/fruitfly/names/",
      "type" : "Hash"
   },
   "tracks" : [
      {
         "chunkSizeLimit" : 5000000,
         "unsafeMouseover" : true,
         "unsafePopup" : true,
         "style" : {
             "color" : "{variationColor}",
             "height" : 8,
	     "description" : "{variationDescription}",
             "label" : "{variationLabel}"
         },
         "glyph" : "{variationGlyph}",
	 "metadata" : {
            "about" : "Phenotypic variants. Features are colored according to the Variant Effect Predictor's predicted impact: red for high, purple for modifier, gold/yellow for medium, and cyan for low."
         },
         "onClick" : {
            "label" : "{variantsBalloon}",
            "url" : "{variationCurieUrl}",
            "title" : "Put a title here",
            "action" : "iframeDialog"
         },
         "label" : "Variants",
         "type" : "CanvasVariants",
         "urlTemplate" : "jbrowse/VCF_R6_82.vcf.gz",
         "storeClass" : "JBrowse/Store/SeqFeature/VCFTabix",
         "key" : "Variants"
      },
      {
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "chunkSize" : 80000,
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/FlyBase/fruitfly/seq/{refseq_dirpath}/{refseq}-",
         "label" : "DNA",
         "type" : "SequenceTrack",
         "compress" : 1,
         "key" : "Reference sequence"
      },
      {
         "glyph" : "{geneGlyph}",
         "impliedUTRs" : true,
         "onClick" : {
            "url" : "{curieUrl}",
            "title" : "Click to open in a full window:",
            "action" : "iframeDialog"
         },
         "style" : {
            "color" : "{geneColor}",
            "height" : "{geneHeight}",
            "className" : "feature"
         },
         "key" : "All Genes",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : "CanvasFeatures",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/docker/3.0.0/FlyBase/fruitfly/tracks/All Genes/{refseq}/trackData.jsonz",
         "compress" : 1,
         "type" : "CanvasFeatures",
         "label" : "All Genes",
         "maxHeight" : 3000
      },
      {
         "label" : "UCSC Conservation",
         "variance_band" : 0,
         "unsafeMouseover" : true,
         "autoscale" : "local",
         "type" : "JBrowse/View/Track/Wiggle/XYPlot",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/UCSC/dm6.27way.phastCons.bw",
         "unsafePopup" : true,
         "key" : "UCSC Conservation",
         "metadata" : {
            "shortInfo" : "UCSC drosophilid conservation calculated across 27 species. For more information about these data, see the <a href='http://hgdownload.soe.ucsc.edu/goldenPath/dm6/phastCons27way/'>README at UCSC</a>. "
         },
         "style" : {
            "height" : 20,
            "pos_color"  : "grey"
         },
         "storeClass" : "JBrowse/Store/SeqFeature/BigWig"
      }
   ],
   "include" : [
      "../functions.conf"
   ],
   "dataset_id" : "fly",
   "formatVersion" : 1
}
